<h1>Sound#index</h1>
<%= link_to "Videos", videos_path %><br>
<%= link_to "Create a new sound", new_sound_path %>
<br><br><br>

<% @sounds.each do |sound| %>
<%= sound.title %> <br>
  <object height="81" width="100%" id="yourPlayerId" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000">
    <param name="movie" value="http://player.soundcloud.com/player.swf?url=<%= sound.soundcloud_url %>&enable_api=true&object_id=yourPlayerId"></param>
    <param name="allowscriptaccess" value="always"></param>
    <embed allowscriptaccess="always" height="81" src="http://player.soundcloud.com/player.swf?url=<%= sound.soundcloud_url %>&enable_api=true&object_id=yourPlayerId" type="application/x-shockwave-flash" width="100%" name="yourPlayerId"></embed>
  </object>
  <br>
<% end %>
